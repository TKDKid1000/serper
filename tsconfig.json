{
  "compilerOptions": {
    "target": "ES2016",
    "module": "UMD",
    "moduleResolution": "Node",
    "outDir": "./lib",
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "paths": {
      "serper": [
        "lib/src/serper.ts"
      ]
    }
  },
  "include": ["src", "types/types.d.ts"],
  "exclude": ["node_modules"]
}
